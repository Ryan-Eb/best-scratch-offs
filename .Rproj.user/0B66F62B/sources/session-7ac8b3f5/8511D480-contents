library(rvest)
library(tidyverse)
library(httr)
library(jsonlite)

ok_url1 <- "https://www.lottery.ok.gov/scratchers/get"

ok_odds <- ok_url1 %>% GET() %>% .$content %>% rawToChar() %>% fromJSON() %>% .$Games %>% 
  select(GameId,Price,OverallOdds,Name,TicketsPrinted)

ok_url2 <- "https://www.lottery.ok.gov/scratchers/prizes"

read_html(ok_url2) %>% html_text2() %>% fromJSON() %>%
  inner_join(ok_odds)

